# Test/Debug Properties
LOG_DB_QUERIES=true
LOG_LEVEL=debug
NODE_ENV=development
NODE_OPTIONS=--max-old-space-size=512

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback
GOOGLE_SCOPES=https://www.googleapis.com/auth/drive.file,https://www.googleapis.com/auth/spreadsheets

# User Access Control
USER_WHITELIST=user1@example.com,user2@example.com

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=data
POSTGRES_USER=postgres
POSTGRES_PASSWORD=pgpw

# Localstack Configuration
AWS_REGION=us-east-2
FILE_LIBRARY_S3_BUCKET=avise-file-library
FILE_LIBRARY_S3_PREFIX=local

# Session Management
SESSION_SECRET=your-session-secret-key

# Frontend Configuration
VITE_API_URL=http://localhost:3000
