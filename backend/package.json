{
  "name": "backend",
  "version": "1.0.0",
  "description": "Business Intelligence Coding Challenge Backend",
  "author": "Avise",
  "license": "UNLICENSED",
  "private": true,
  "packageManager": "pnpm@8.15.7",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "check": "concurrently \"pnpm:check:format\" \"pnpm:check:lint\" \"pnpm:check:type\"",
    "check:format": "prettier -c 'src/**/*.ts' 'scripts/**/*.ts'",
    "check:lint": "eslint --max-warnings=0 src --ext .ts,.js",
    "check:type": "tsc -p ./tsconfig.json",
    "check:ready": "pnpm fix:format && pnpm check",
    "complete-refresh": "TS_NODE_FILES=true ts-node scripts/setupHelper/apiSetupHelper.ts --completeRefresh",
    "create-migration": "TS_NODE_FILES=true ts-node scripts/createMigration.ts",
    "drop-remake-db": "docker exec -it backend-postgres-1 psql -U postgres -d data -c 'drop schema public cascade; create schema public' && pnpm run migrate-local",
    "fix": "pnpm fix:lint & pnpm fix:format",
    "fix:format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts'",
    "fix:lint": "eslint --max-warnings=0 src --ext .ts,.js --fix",
    "generate-data": "TS_NODE_FILES=true ts-node -r dotenv/config scripts/generateData.ts",
    "initial-setup": "TS_NODE_FILES=true ts-node scripts/setupHelper/apiSetupHelper.ts --initialSetup",
    "migrate-down-one": "db-migrate down -c 1",
    "migrate-local": "db-migrate up",
    "postbuild": "copyfiles -u 1 src/**/*.html src/**/*.txt lib/",
    "postinstall": "pnpm exec prisma generate",
    "prebuild": "rm -rf lib/ ",
    "reset-db": "pnpm run drop-remake-db && pnpm run migrate-local",
    "setup": "pnpm install && pnpm run migrate-local",
    "start": "TS_NODE_FILES=true pnpm exec ts-node -r dotenv/config src/app.ts",
    "start:build": "node ./lib/app.js",
    "start:dev": "nodemon --watch src -e ts --exec pnpm run start"
  },
  "dependencies": {
    "@dinero.js/currencies": "2.0.0-alpha.14",
    "@prisma/adapter-pg": "5.18.0",
    "@prisma/client": "5.18.0",
    "axios": "1.7.4",
    "axios-retry": "4.4.2",
    "blend-promise-utils": "1.29.0",
    "chalk": "4.1.2",
    "cors": "2.8.5",
    "csv-parse": "5.5.6",
    "csv-stringify": "6.5.1",
    "date-fns": "4.1.0",
    "db-migrate": "0.11.14",
    "db-migrate-pg": "1.5.2",
    "dd-trace": "5.24.0",
    "dinero.js": "1.9.1",
    "email-validator": "^2.0.4",
    "exceljs": "^4.4.0",
    "express": "4.20.0",
    "express-promise-router": "4.1.1",
    "joi": "17.13.3",
    "mime-types": "2.1.35",
    "morgan": "1.10.0",
    "openai": "4.80.1",
    "pdfjs-dist": "3.11.174",
    "pg": "8.12.0",
    "uuid": "10.0.0",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@clack/prompts": "0.7.0",
    "@faker-js/faker": "9.0.3",
    "@paralleldrive/cuid2": "2.2.2",
    "@types/chai": "4.3.16",
    "@types/chai-datetime": "1.0.0",
    "@types/cors": "2.8.17",
    "@types/deep-equal-in-any-order": "1.0.3",
    "@types/dinero.js": "1.9.4",
    "@types/express": "4.17.21",
    "@types/json2csv": "5.0.7",
    "@types/lodash": "4.17.7",
    "@types/luxon": "3.4.2",
    "@types/mocha": "10.0.6",
    "@types/morgan": "1.9.9",
    "@types/pg": "8.11.6",
    "@types/sinon": "17.0.3",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@types/yargs": "17.0.32",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "chai": "4.5.0",
    "chai-datetime": "1.8.1",
    "concurrently": "9.0.1",
    "copyfiles": "2.4.1",
    "database-cleaner": "1.3.0",
    "deep-equal-in-any-order": "2.0.6",
    "dotenv": "16.4.5",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-functional": "6.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-unicorn": "53.0.0",
    "json2csv": "6.0.0-alpha.2",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "mocha": "10.7.0",
    "node-mocks-http": "1.15.1",
    "nodemon": "3.1.4",
    "prisma": "5.18.0",
    "sinon": "18.0.0",
    "supertest": "7.0.0",
    "yargs": "17.7.2"
  },
  "prettier": {
    "printWidth": 100,
    "overrides": [
      {
        "files": "**/*.yaml",
        "options": {
          "singleQuote": true
        }
      }
    ]
  }
}
